		</attribute>
		<attribute>
			<description>Indica o name do componente a ser incorporado a mascara</description>
			<name>attachTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica se o preenchimento deve ocorrer de tras pra frente</description>
