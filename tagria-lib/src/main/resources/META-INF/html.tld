<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">

	<description>Tagria html library</description>
	<display-name>Tagria html library</display-name>
	<tlib-version>3.0</tlib-version>
	<short-name>html</short-name>
	<uri>http://tagrialib.jslsolucoes.com/tags/html</uri>


	<tag>
		<description>Renderiza um bloco de código fonte de uma determinada linguagem sem executá-la</description>
		<display-name>Misc</display-name>
		<name>code</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.CodeTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:code&gt;
				private class Java {
					private String id;
					private String name;
				}
			&lt;/html:code&gt; </example>
			
	</tag>

	<tag>
		<description>Renderiza um bloco css</description>
		<display-name>Css</display-name>
		<name>cssCode</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.CssCodeTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:cssCode&gt;
			.red  {
				border :1px solid red;
			}
			&lt;/html:cssCode&gt;
			&lt;html:div cssClass="red"&gt;
			    Conteudo
			&lt;/html:div&gt; </example>
	</tag>

	<tag>
		<description>Renderiza uma tabela</description>
		<display-name>Table</display-name>
		<name>table</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.TableTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:table&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn>Coluna 1&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn>Valor 1&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
			&lt;/html:table>
		</example>
	</tag>

	<tag>
		<description>Renderiza uma linha de uma tabela</description>
		<display-name>Table</display-name>
		<name>tableLine</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.TableLineTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica se o componente deve ser renderizado</description>
			<name>rendered</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<example>
			&lt;html:table&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn>Coluna 1&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn>Valor 1&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
			&lt;/html:table>
		</example>
	</tag>

	<tag>
		<description>Renderiza uma coluna de tabela</description>
		<display-name>Table</display-name>
		<name>tableColumn</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.TableColumnTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica se a coluna deve ser renderizada com um destaque</description>
			<name>header</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>Indica um format em que o value deve ser convertido . Opções : date,timestamp,hour,currency,cep,cnpj,tel,cpf</description>
			<name>format</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o numero de colunas que essa coluna deve preencher</description>
			<name>colspan</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Integer</type>
		</attribute>
		<example>
			&lt;html:table&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn header="true">Coluna 1&lt;/html:tableColumn&gt;
					&lt;html:tableColumn header="true">Coluna 2&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn>Valor 1&lt;/html:tableColumn&gt;
					&lt;html:tableColumn>Valor 2&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
				&lt;html:tableLine&gt;
					&lt;html:tableColumn colspan="2">Valor 3&lt;/html:tableColumn&gt;
				&lt;/html:tableLine&gt;
			&lt;/html:table>
		</example>
	</tag>

	<tag>
		<description>Renderiza um evento disparado para um determinado componente</description>
		<display-name>Javascript</display-name>
		<name>jsEvent</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.JsEventTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o name do componente a ser atingido</description>
			<name>attachTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o evento javascript a ser disparado</description>
			<name>event</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:input name="input1" placeholder="Entre e saia do campo" /&gt;
			
			&lt;html:jsEvent attachTo="input1" event="blur"&gt;
			    alert('blur');
			&lt;/html:jsEvent&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um bloco modal</description>
		<display-name>Misc</display-name>
		<name>modal</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.ModalTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o id do modal para manipulacao direta de eventos</description>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o label do titulo do modal</description>
			<name>label</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o componente que dispara a tela de modal</description>
			<name>attachTo</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
		    &lt;html:button id="botao" label="Abrir modal"/&gt;
			&lt;html:modal label="Titulo do modal" attachTo="botao"&gt;
				Outros componentes 
			&lt;/html:modal&gt; </example>
	</tag>


	<tag>
		<description>Renderiza um picker de horario para um componente</description>
		<display-name>Picker</display-name>
		<name>timePicker</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.TimePickerTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o componente que dispara o componente</description>
			<name>attachTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:input name="input1" placeholder="Horario" /&gt;
			&lt;html:timePicker attachTo="input1"/&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um dateTimePicker para um componente</description>
		<display-name>Picker</display-name>
		<name>dateTimePicker</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.DateTimePickerTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o componente que dispara o componente</description>
			<name>attachTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:input name="input1" placeholder="Data e horario" /&gt;
			&lt;html:dateTimePicker attachTo="input1"/&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um datePicker para um componente</description>
		<display-name>Picker</display-name>
		<name>datePicker</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.DatePickerTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o componente que dispara o componente</description>
			<name>attachTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o dia mínimo para escolha da data</description>
			<name>min</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Integer</type>
		</attribute>
		<example>
			&lt;html:input name="input1" placeholder="Data" /&gt;
			&lt;html:datePicker attachTo="input1" min="0"/&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um evento ao selecionar uma data</description>
		<display-name>Picker</display-name>
		<name>datePickerOnSelect</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.DatePickerOnSelectTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:input name="input1" placeholder="Data" /&gt;
			&lt;html:datePicker attachTo="input1" min="0"&gt;
				&lt;html:datePickerOnSelect&gt;
					/* Nesse ponto fica disponivel as variaveis dateText,inst  */
					alert(dateText);
				&lt;/html:datePickerOnSelect&gt;
			&lt;/html:datePicker&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um icone</description>
		<display-name>Image</display-name>
		<name>icon</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.IconTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o icone a ser renderizado. Lista disponível em http://getbootstrap.com/components/#glyphicons</description>
			<name>icon</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:icon icon="cog" /&gt; </example>
	</tag>

	<tag>
		<description>Renderiza a view com todo cabeçalho . Essa view deve ser declarada APENAS UMA VEZ e como tag principal em toda view utilizada</description>
		<display-name>Misc</display-name>
		<name>view</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.ViewTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o title da pagina corrente</description>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:view&gt;
				Todos os componentes necessários
			&lt;/html:view&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um bloco de container principal com uma margem padrao</description>
		<display-name>Layout</display-name>
		<name>container</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.ContainerTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica se o container deve ser responsivo para 100% da tela</description>
			<name>fluid</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<example>
			
			&lt;html:alert state="info"&gt;
				Componente fora do container
			&lt;/html:alert&gt;
			
			&lt;html:container&gt;
				&lt;html:alert state="info"&gt;
					Componente dentro do container
				&lt;/html:alert&gt;
			&lt;/html:container&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um painel</description>
		<display-name>Panel</display-name>
		<name>panel</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.PanelTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o estado do painel. Default : primary. Opções : success,danger,info,default,primary</description>
			<name>state</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:panel&gt;
				&lt;html:panelHead label="Titulo"/&gt;
				&lt;html:panelBody&gt;
					Outros componentes
				&lt;/html:panelBody&gt;
			&lt;/html:panel>
		</example>
	</tag>

	<tag>
		<description>Renderiza um titulo no painel</description>
		<display-name>Panel</display-name>
		<name>panelHead</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.PanelHeadTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o titulo do painel</description>
			<name>label</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<example>
			&lt;html:panel&gt;
				&lt;html:panelHead label="Titulo"/&gt;
				&lt;html:panelBody&gt;
					Outros componentes
				&lt;/html:panelBody&gt;
			&lt;/html:panel>
		</example>
	</tag>

	<tag>
		<description>Renderiza o corpo do painel</description>
		<display-name>Panel</display-name>
		<name>panelBody</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.PanelBodyTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:panel&gt;
				&lt;html:panelHead label="Titulo"/&gt;
				&lt;html:panelBody&gt;
					Outros componentes
				&lt;/html:panelBody&gt;
			&lt;/html:panel>
		</example>
	</tag>

	<tag>
		<description>Renderiza uma caixa de alerta</description>
		<display-name>Misc</display-name>
		<name>alert</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.AlertTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica se o componente pode ser fechada. Default : false</description>
			<name>dismissible</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>Indica o estado em que a caixa deve ser renderizada. Default : warning. Opções : warning,success,danger,info</description>
			<name>state</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica se o componente deve ser renderizado. Default : true</description>
			<name>rendered</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<example>
			&lt;html:alert state="info"&gt;
				Caixa de informação
			&lt;/html:alert&gt; </example>
	</tag>



	<tag>
		<description>Renderiza um formulario</description>
		<display-name>Form</display-name>
		<name>form</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.FormTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica a action de submissao</description>
			<name>action</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o method a ser utilizado. Default : post. Opções : post,get</description>
			<name>method</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica uma validacao a ser executada com os dados do formulario</description>
			<name>validation</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o label do formulario</description>
			<name>label</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica se o componente deve ser renderizado</description>
			<name>rendered</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>Indica se o formulario deve possuir suporte a arquivos</description>
			<name>multipart</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<example>
		
			&lt;html:form action="#"&gt;
				&lt;html:formGroup label="Campo 1" required="true"&gt;
					&lt;html:input required="true" name="field1"/&gt;
				&lt;/html:formGroup&gt;
				&lt;html:formGroup label="Campo 2"&gt;
					&lt;html:input name="field2"/&gt;
				&lt;/html:formGroup&gt;
			&lt;/html:form&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um agrupamento de componente no formulario</description>
		<display-name>Form</display-name>
		<name>formGroup</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.FormGroupTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o label do agrupamento</description>
			<name>label</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o id do agrupamento</description>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica o elemento a ser orientado o label</description>
			<name>forElement</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica se o preenchimento do componente interno deve ser obrigatório</description>
			<name>required</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>Indica se o componente deve ser renderizado</description>
			<name>rendered</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>Indica se o componente dever ser visivel</description>
			<name>visible</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<example>
			&lt;html:form action="#"&gt;
				&lt;html:formGroup forElement="field1" label="Campo 1" required="true"&gt;
					&lt;html:input required="true" name="field1"/&gt;
				&lt;/html:formGroup&gt;
				&lt;html:formGroup label="Campo 2"&gt;
					&lt;html:input name="field2"/&gt;
				&lt;/html:formGroup&gt;
			&lt;/html:form&gt; </example>
	</tag>

	<tag>
		<description>Renderiza uma linha de subdivisao de colunas (dividida em 12 seções combinada com a tag col)</description>
		<display-name>Form</display-name>
		<name>row</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.RowTag</tag-class>
		<body-content>scriptless</body-content>
		<example>
			&lt;html:form action="#"&gt;
				&lt;html:formGroup forElement="field1" label="Campo 1" required="true"&gt;
					&lt;html:row&gt;
						&lt;html:col size="6"&gt;
							&lt;html:input required="true" name="field1"/&gt;
						&lt;/html:col&gt;
						&lt;html:col size="6"&gt;
							&lt;html:input name="field2"/&gt;
						&lt;/html:col&gt;
					&lt;/html:row>
				&lt;/html:formGroup&gt;
			&lt;/html:form&gt; </example>
	</tag>

	<tag>
		<description>Renderiza uma coluna dentro de uma linha</description>
		<display-name>Form</display-name>
		<name>col</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.ColTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica o número de colunas a preencher . Opções : 1 a 12</description>
			<name>size</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Integer</type>
		</attribute>
		<example>
			&lt;html:form action="#"&gt;
				&lt;html:formGroup forElement="field1" label="Campo 1" required="true"&gt;
					&lt;html:row&gt;
						&lt;html:col size="3"&gt;
							&lt;html:input required="true" name="field1"/&gt;
						&lt;/html:col&gt;
						&lt;html:col size="9"&gt;
							&lt;html:input name="field2"/&gt;
						&lt;/html:col&gt;
					&lt;/html:row>
				&lt;/html:formGroup&gt;
			&lt;/html:form&gt; </example>
	</tag>

	<tag>
		<description>Renderiza um input</description>
		<display-name>Input</display-name>
		<name>input</name>
		<tag-class>com.jslsolucoes.tagria.lib.tag.html.InputTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>Indica uma classe css para o componente</description>
			<name>cssClass</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Indica um title para o componente</description>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
